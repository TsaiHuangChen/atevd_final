<%= form_for @device, :url => devices_path do |f| %>

	<div class="page-header">
		<h1>Detection Rules <small>New</small></h1>
	</div>
	<%= render :partial => 'form', :locals => { :f => f } %>

	<br>
	<%= f.submit "Submit" %>
	<%= link_to 'back to device index', :controller => 'devices', :action => 'index' %>

<% end %>